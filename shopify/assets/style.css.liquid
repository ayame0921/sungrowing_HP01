.header-nav ul {
display:-webkit-box;
display:-ms-flexbox;
display: flex;
-webkit-box-pack: justify;
-ms-flex-pack: justify;
justify-content: space-between;
}
.header-nav ul li a {
position: relative;
}
/*ホバーエフェクト*/
.header-nav ul li a::after {
/*マークのスタイル*/
position: absolute;
content: "";
display: block;
width: 10px;
height: 10px;
background-color: #000000;
color: #000000;
border-radius: 50%;
top: -15px;
left: calc(50% - 5px);
/*不透明度0で非表示*/
opacity: 0;
/*アニメーションの速度設定*/
-webkit-transition: all 0.6s ease;
transition: all 0.6s ease;
}
.header-nav ul li a:hover {
    color: #000000;
    text-decoration: none;
}
.header-nav ul li a:hover::after {
/*不透明度1で表示*/
opacity: 1;
}



